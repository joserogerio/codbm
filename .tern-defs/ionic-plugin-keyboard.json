{
    "!name": "ionic-plugin-keyboard",
    "!define": {
        "!modules": {
            "keyboard": {
                "hideKeyboardAccessoryBar": "fn(hide: ?)",
                "close": "fn()",
                "show": "fn()",
                "disableScroll": "fn(disable: ?)",
                "isVisible": "bool",
                "!type": "fn()"
            },
            "KeyboardProxy": {
                "disableScroll": "fn(disable: ?)",
                "show": "fn()",
                "close": "fn()"
            }
        }
    },
    "!plugin-version": "1.0.8",
    "cordova": {
        "plugins": {
            "Keyboard": "!modules.keyboard"
        }
    }
}